                {"id": "P.PPLS", "name": "Populated places (multiple settlements)"},
                {"id": "P.PPLX", "name": "Section of populated place"},
                {"id": "P.PPLQ", "name": "Abandoned populated place"},
                {"id": "P.PPLH", "name": "Historical populated place"},
                {"id": "A.ADM1", "name": "First-order admin division (state, province, region)"},
