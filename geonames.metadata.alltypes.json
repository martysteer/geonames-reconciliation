        "geonames": {
          "title": "GeoNames",
          "description": "Geographic place names for reconciliation",
          "fts_table": "geonames_fts",
          "fts_pk": "rowid",
          "plugins": {
            "datasette-reconcile": {
              "id_field": "id",
              "name_field": "name",
              "type_field": "type",
              "description_field": "country_code",
              "type_default": [
                {"id": "P.PPL", "name": "Populated place (city, town, village)"},
                {"id": "P.PPLA", "name": "Seat of first-order admin division (state/province capital)"},
                {"id": "P.PPLA2", "name": "Seat of second-order admin division (county seat)"},
